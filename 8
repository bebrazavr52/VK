#coding: utf-8
a = input()
b = input()
c = input()
x = 1
d = max(a)
e = max(b)
f = max(c)
if max(b) == d:
    print (d)
else:
    max(b) == e
    print(e)
if max(c) == d:
    print (d)
else:
    max(c) == f
    print (f)
if max(c) == e:
    print (e)
else:
    max(c) == f
    print (f)
x = [d,e,f]
x.sort(reverse=True)
x = str(x)
x=x.replace(" ",";")
x=x.replace(",","")
x=x.replace("'","")
print (x)
